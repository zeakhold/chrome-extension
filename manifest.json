{
  "manifest_version": 2,
  "name": "天气预报",
  "version": "1.2.0",
  "description": "一款Chrome天气预报插件，提供六天天气预报(包括昨天)，显示天气、温度、风速及当天空气质量等信息。",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "天气预报",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://node.zeakhold.com/*"
  ]
}